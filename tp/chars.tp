require {
    print_char, println_char, println_str, println_int, print_int
}

fn test(x: char, y: char) char {
    return (x as int + y as int) as char;
}

fn ppp(a: char[]) void {
    print_int(a$length);
    println_str(a);
}

fn main() int {
    a: char = '啊';
    b: char[] = "abc";

    ppp(b);
    println_char(test(a, 'x'));

    return b[1] as int;
}