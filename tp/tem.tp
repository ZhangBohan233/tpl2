import lang

require {
    malloc
}

class A {
    xx: int;

    fn __init__(this: *A) void {
        super.__init__(this);
        this.xx = 1;
    }

    fn foo(this: *A, x: int) int {
        return this.xx + x;
    }
}

class B(A) {
    fn __init__(this: *B) void {
        super.__init__(this);

        this.xx = 6;
    }
}

fn main() int {
    aa: *B = new B();
    aa.__init__();
    return aa.foo(2);
}
