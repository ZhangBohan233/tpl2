import lang

require {
    malloc
}

class A {
    xx: int;

    fn __init__(this: *A) void {
        //super.__init__(this);
        this.xx = 1;
    }

    fn foo(this: *A, x: int) int {
        return this.xx + x;
    }
}

fn main() int {
    aa: *A = new A();
    aa.__init__();
    return aa.foo(2);
}
